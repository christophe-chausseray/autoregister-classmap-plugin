{
    "name": "chris13/autoregister-classmap-plugin",
    "description": "Plugin composer to register external source code as PSR4.",
    "keywords": ["composer", "autoregister classmap"],
    "type": "composer-plugin",
    "license": "proprietary",
    "authors": [
        {
            "name": "Christophe Chausseray",
            "email": "chausseray.christophe@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.3",
        "composer-plugin-api": "^1.0",
        "symfony/yaml": "~2.3",
        "neirda24/tools-bundle": "^2.0"
    },
    "require-dev": {
        "composer/composer": "~1.0@dev",
        "symfony/phpunit-bridge": "^2.7",
        "phpunit/phpunit": "~5.1",
        "phake/phake": "~2.2"
    },
    "autoload": {
        "psr-4": {
            "Chris\\Composer\\AutoregisterClassmapPlugin\\": "lib/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "": "tests/"
        }
    },
    "extra": {
        "class": "Chris\\Composer\\AutoregisterClassmapPlugin\\AutoregisterClassmapPlugin"
    }
}
